package hust.namtran.Algorithms;

public class GreatestCommonDivisor {
	/*
	 * Tìm ước chung lớn nhất
	 */
	public static void main(String[] args) {

	}

	private static int gcd(int a, int b) {
		int upper, lower;
		if (a >= b) {
			upper = a;
			lower = b;
		} else {
			upper = b;
			lower = a;
		}
	}

}
