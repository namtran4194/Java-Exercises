package hust.namtran.FlowControls;

public class PrintPattern {
	
	public static void main(String[] args) {
		int sizeTri = 8;
		/*
		 * print a square triangle
		 */
		for (int row = 1; row <= sizeTri; row++) {
			for (int col = 1; col <= row; col++) {
				System.out.print("#");
				if (col != row)
					System.out.print(" ");
			}
			System.out.println();
		}
		/*
		 * print a round
		 */
	}

}
