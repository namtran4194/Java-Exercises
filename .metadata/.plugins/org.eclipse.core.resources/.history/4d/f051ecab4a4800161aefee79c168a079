package adapters;

import java.awt.Graphics;

/**
 * The Cell class models each individual cell of the game board.
 */
public class Cell {
	Seed content;
	int row, col;

	public Cell(int row, int col) {
		this.row = row;
		this.col = col;
		clear();
	}

	public void clear() {
		content = Seed.EMPTY;
	}

	public void paint(Graphics g) {

	}
}
